{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "PractiseADF-001"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"LS_Source_DERIVED_Trans_Dataflow_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Source_DERIVED_Trans_Dataflow'"
		},
		"LS_Keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-demo-101.vault.azure.net/"
		},
		"LS_SQLKeyVault_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "pocsqlserver001.database.windows.net"
		},
		"LS_SQLKeyVault_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "PocSqlDatabase001"
		},
		"LS_SQLKeyVault_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Roshan"
		},
		"trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/14b54a77-42f5-40d1-8202-360631e7ec2c/resourceGroups/Naresh-RG-01/providers/Microsoft.Storage/storageAccounts/srcsa2024"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "PractiseADF-001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"LS_Source_DERIVED_Trans_Dataflow_connectionString": {
						"value": "[parameters('LS_Source_DERIVED_Trans_Dataflow_connectionString')]"
					},
					"LS_Keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_Keyvault_properties_typeProperties_baseUrl')]"
					},
					"LS_SQLKeyVault_properties_typeProperties_server": {
						"value": "[parameters('LS_SQLKeyVault_properties_typeProperties_server')]"
					},
					"LS_SQLKeyVault_properties_typeProperties_database": {
						"value": "[parameters('LS_SQLKeyVault_properties_typeProperties_database')]"
					},
					"LS_SQLKeyVault_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQLKeyVault_properties_typeProperties_userName')]"
					},
					"trigger_properties_typeProperties_scope": {
						"value": "[parameters('trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}